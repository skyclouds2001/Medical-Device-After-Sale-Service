{
  "pages": [
    "pages/register/register",
    "pages/index/index",
    "pages/index/search-products/search-products",
    "pages/fix/fix",
    "pages/fix/common-issues/common-issues",
    "pages/mine/mine"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "医疗器械售后服务",
    "navigationBarTextStyle": "black",
    "initialRenderingCache": "static",
    "handleWebviewPreload": "auto"
  },
  "tabBar": {
    "list": [
      {
        "iconPath": "icons/home_fill.png",
        "selectedIconPath": "icons/home_fill_selected.png",
        "text": "首页",
        "pagePath": "pages/index/index"
      },
      {
        "iconPath": "icons/repair_fill.png",
        "selectedIconPath": "icons/repair_fill_selected.png",
        "text": "设备自检/客服",
        "pagePath": "pages/fix/fix"
      },
      {
        "iconPath": "icons/wode_fill.png",
        "selectedIconPath": "icons/wode_fill_selected.png",
        "text": "个人中心",
        "pagePath": "pages/mine/mine"
      }
    ],
    "backgroundColor": "#FFFFFF",
    "selectedColor": "#2D3DAA",
    "color": "#303030",
    "custom": true
  },
  "sitemapLocation": "sitemap.json",
  "usingComponents": {
    "tab-bar": "/custom-tab-bar/index"
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "lazyCodeLoading": "requiredComponents",
  "enablePassiveEvent": true,
  "resolveAlias": {
    "@/*": "/*"
  },
  "debug": true,
  "debugOptions": {
    "enableFPSPanel": true
  }
}