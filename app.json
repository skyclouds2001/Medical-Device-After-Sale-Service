{
  "pages": [
    "pages/register/index",
    "pages/home/index",
    "pages/product-list/index",
    "pages/workorder/index",
    "pages/software/index",
    "pages/submited/index",
    "pages/document-list/index",
    "pages/mine/index",
    "pages/history/index",
    "pages/workorderdetail/index"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "医疗器械售后服务",
    "navigationBarTextStyle": "black",
    "initialRenderingCache": "static",
    "handleWebviewPreload": "auto"
  },
  "tabBar": {
    "list": [
      {
        "iconPath": "icons/home_fill.png",
        "selectedIconPath": "icons/home_fill_selected.png",
        "text": "首页",
        "pagePath": "pages/home/index"
      },
      {
        "iconPath": "icons/wode_fill.png",
        "selectedIconPath": "icons/wode_fill_selected.png",
        "text": "个人中心",
        "pagePath": "pages/mine/index"
      }
    ],
    "backgroundColor": "#FFFFFF",
    "selectedColor": "#2D3DAA",
    "color": "#303030",
    "custom": true
  },
  "sitemapLocation": "sitemap.json",
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "lazyCodeLoading": "requiredComponents",
  "enablePassiveEvent": true,
  "resolveAlias": {
    "@/*": "/*"
  },
  "debug": true,
  "debugOptions": {
    "enableFPSPanel": true
  }
}